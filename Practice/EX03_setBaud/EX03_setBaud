#include <DynamixelShield.h>
DynamixelShield dxl;

void setup()
{
  // put your setup code here, to run once:
  for(int ii=0;ii<=417;ii++)
  {
    dxl.begin(2400 * ii);
    delay(10);
    dxl.addMotor(1, M_XL430);// M_AX12, M_AX18, M_MX28, M_XL430 .....
    dxl.torqueOff(1);
    dxl.setBaud(1,3);
    dxl.setBaud(1,1);
  }
  dxl.begin(1000000);
  delay(10);
  dxl.addMotor(1, M_XL430);// M_AX12, M_AX18, M_MX28, M_XL430 .....
  dxl.ledOn(1);
}

void loop()
{
}
